{
  "name": "fluxfeed-app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 5173 --strictPort",
    "server": "tsx watch server/index.ts",
    "dev:open": "wait-on http://localhost:5173 && powershell -Command start http://localhost:5173",
    "dev:all": "concurrently \"npm:server\" \"vite\" \"npm:dev:open\"",
    "build": "tsc -b && vite build",
    "preview": "vite preview --port 5173",
    "init-db": "tsx server/init-db.ts"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router-dom": "^7.9.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.7.5",
    "@types/pg": "^8.15.6",
    "@types/react": "18.3.8",
    "@types/react-dom": "18.3.0",
    "@vitejs/plugin-react": "4.3.3",
    "autoprefixer": "10.4.20",
    "concurrently": "9.0.1",
    "postcss": "8.4.47",
    "tailwindcss": "3.4.14",
    "tsx": "4.19.2",
    "typescript": "5.6.3",
    "vite": "5.4.8",
    "wait-on": "7.2.0"
  }
}
